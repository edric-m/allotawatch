<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="false"
    android:layout_centerVertical="true">
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout_contact"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".ActivityInfo$PlaceholderFragment"
    android:theme="@style/FormTheme">

    <Button
        android:id="@+id/section_send_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/send"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <TextView
        android:id="@+id/section_label1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@id/section_send_button"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:id="@+id/section_label2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/section_label1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <EditText
        android:id="@+id/section_input2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/section_label2"
        app:layout_constraintLeft_toLeftOf="parent"
        android:visibility="invisible"
        android:maxLength="200"/>

    <TextView
        android:id="@+id/section_label3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/section_input2"
        app:layout_constraintLeft_toLeftOf="parent" />

    <EditText
        android:id="@+id/section_input3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/section_label3"
        app:layout_constraintLeft_toLeftOf="parent"
        android:visibility="invisible"
        android:maxLength="200"/>

    <TextView
        android:id="@+id/section_label4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/section_input3"
        app:layout_constraintLeft_toLeftOf="parent" />

    <EditText
        android:id="@+id/section_input4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/section_label4"
        app:layout_constraintLeft_toLeftOf="parent"
        android:visibility="invisible"
        android:maxLength="200"/>

    <TextView
        android:id="@+id/section_label5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/section_input4"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <EditText
        android:id="@+id/section_input5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/section_label5"
        app:layout_constraintLeft_toLeftOf="parent"
        android:visibility="invisible"
        android:maxLength="200"/>

    <TextView
        android:id="@+id/section_label6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/section_input5"
        app:layout_constraintLeft_toLeftOf="parent" />

    <EditText
        android:id="@+id/section_input6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/section_label6"
        app:layout_constraintLeft_toLeftOf="parent"
        android:visibility="invisible"
        android:maxLength="200"/>

    <TextView
        android:id="@+id/section_label7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/section_input6"
        app:layout_constraintLeft_toLeftOf="parent" />

    <EditText
        android:id="@+id/section_input7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/section_label7"
        app:layout_constraintLeft_toLeftOf="parent"
        android:visibility="invisible"
        android:maxLength="200"/>

    <TextView
        android:id="@+id/section_label8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/section_input7"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <EditText
        android:id="@+id/section_input8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/section_label8"
        app:layout_constraintLeft_toLeftOf="parent"
        android:visibility="invisible"
        android:maxLength="200"/>

    <TextView
        android:id="@+id/section_label9"
        android:layout_width="wrap_content"
        android:layout_height="300dp"
        app:layout_constraintTop_toBottomOf="@+id/section_input7"
        app:layout_constraintLeft_toLeftOf="parent"/>

</android.support.constraint.ConstraintLayout>
</ScrollView>